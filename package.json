{
  "name": "react-leaflet-draw-ts",
  "version": "0.0.1",
  "description": "React component build on top of React-Leaflet that integrate leaflet-draw feature with typescript",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run example && http-serve ./example",
    "example": "rimraf ./example/src/js && webpack --config ./example/webpack.config.js -p",
    "convertjs": "rimraf lib && tsc -p ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tumerorkun/react-leaflet-draw-ts.git"
  },
  "keywords": [
    "react",
    "react-leaflet",
    "leaflet",
    "leaflet-draw",
    "draw",
    "typescript",
    "ts"
  ],
  "author": "Orkun Tumer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tumerorkun/react-leaflet-draw-ts/issues"
  },
  "homepage": "https://github.com/tumerorkun/react-leaflet-draw-ts#readme",
  "devDependencies": {
    "@types/leaflet-draw": "^0.4.13",
    "@types/react": "^15.6.2",
    "@types/react-dom": "^15.5.4",
    "@types/react-leaflet": "^1.1.6",
    "awesome-typescript-loader": "^5.2.0",
    "http-serve": "^1.0.1",
    "leaflet": "^1.3.3",
    "leaflet-draw": "^1.0.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-leaflet": "^1.9.1",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "typescript": "^2.8.3",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  }
}
